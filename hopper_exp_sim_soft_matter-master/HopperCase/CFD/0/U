FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    walla
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    wallb
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    walls
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
    hopper
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
}


